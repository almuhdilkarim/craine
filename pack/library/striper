#!/bin/bash


set -e



if [[ -z $distro_protocol ]];then
    distro_protocol=101
fi

if [[ -z $distro_releases ]];then
    distro_releases="falcone"
fi



## USER PROJECTS
if [[ ! -d "$HOME/.local/craine/" ]]; then
    mkdir -p "$HOME/.local/craine/"
    mkdir -p "$HOME/.local/craine/{project,profile}"
fi



## LOAD LIBRARY
source "$appdirs/library/hackers"
source "$appdirs/library/desktop"
source "$appdirs/library/termine"



## LOAD PATHERS
appmods="$appdirs/modul"
appdist="$appdirs/distros"
appbins="$appdirs/source"
apprule="$appdirs/protoc"
appview="$appdirs/viewer"


## LOAD MODULES