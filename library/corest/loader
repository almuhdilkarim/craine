#!/bin/env bash

set -e


## CRAINE ENVI
craine_corest="$craine_libs/corest"
craine_config="$craine_libs/config"
craine_worker="$HOME/.local/craine"


## CRAINE USER
craine_protos="$craine_worker/protos"
craine_preset="$craine_worker/preset"
craine_proset="$craine_worker/proset"
craine_prolab="$craine_worker/prolab"
craine_preloc="$craine_worker/preloc"


## CRAINE LIBS
source "$craine_corest/auxele"
source "$craine_corest/cordat"
source "$craine_corest/coredi"
source "$craine_corest/corins"
source "$craine_corest/coriso"
source "$craine_corest/corlab"
source "$craine_corest/viewde"
source "$craine_corest/viewda"


## CRAINE PREP
mkdir -p $craine_worker
mkdir -p $craine_preset
mkdir -p $craine_protos
mkdir -p $craine_prolab
mkdir -p $craine_preloc


## CONFIG PATH
craine_banner="$craine_config/banner"


## CONFIG LIBS
source "$craine_config/master"
source "$craine_config/codexs"


## LOADER DEFS
echo preset
craine_preset_basics
echo proset
craine_proset_basics
echo protos
craine_protos_basics