#!/bin/env bash
# Craine skeleton loader

set -e

## LIBRARY PATHVARS
craine_assets="$craine_libs/assets"
craine_corest="$craine_libs/corest"
craine_config="$craine_libs/config"
craine_operat="$craine_libs/operat"
craine_helper="$craine_libs/auxele"
craine_worker="$HOME/.local/craine"
craine_tokens="$craine_worker/tokens"




## CONFIG LIBRARY
source "$craine_config/labrom"
source "$craine_config/corest"


## AUXELE LIBRARY
source "$craine_helper/assets"
source "$craine_helper/colors"
source "$craine_helper/config"
source "$craine_helper/cloner"
source "$craine_helper/deploy"
source "$craine_helper/logger"
source "$craine_helper/seders"
source "$craine_helper/secure"


## COREST LIBRARY
source "$craine_corest/preset"
source "$craine_corest/proset"
source "$craine_corest/protos"


## OPERATION MODE
source "$craine_operat/graphi"
source "$craine_operat/termin"


## WORKS DIRECTORY
craine_protos="$craine_worker/protos"
craine_preset="$craine_worker/preset"
craine_proset="$craine_worker/proset"
craine_prolab="$craine_worker/prolab"
craine_preloc="$craine_worker/preloc"
craine_precon="$craine_worker/config"


## DEFAULT PACKAGERS
craine_locpre="$craine_preset/default"
craine_locpro="$craine_proset/default"
craine_locprt="$craine_protos/default"


## ASSET DIRECTORY
craine_banner="$craine_assets/banner.txt"
craine_banner="$craine_assets/brands.png"


## DEFAULT REPOSITORY
craine_gitpre="https://github.com/almuhdilkarim/preset.git"
craine_gitpro="https://github.com/almuhdilkarim/proset.git"
craine_gitprt="https://github.com/almuhdilkarim/protos.git"


## DAEMON BOOTLOADER
source "$craine_helper/welbot"
