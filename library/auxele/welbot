#!/bin/env bash
# Autoload function


set -e

## CRAINE DIRECTORY
mkdir -p $craine_tokens


## WORKER DIRECTORY
mkdir -p $craine_worker
mkdir -p $craine_preset
mkdir -p $craine_protos
mkdir -p $craine_prolab
mkdir -p $craine_preloc

## TOKENS DIRECTORY
mkdir -p $craine_tokens/preset
mkdir -p $craine_tokens/proset
mkdir -p $craine_tokens/protos

## DEFAULT LOADER  
craine_preset_basics &&
craine_proset_basics &&
craine_protos_basics &&
craine_locals_random