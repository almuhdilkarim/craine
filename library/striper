#!/bin/bash

set -e


## LOAD PATHERS


applibs="$apppath/library"
appsrcs="$apppath/source"




## LOAD INTERFACE
source "$apppath/interfe/hackers"
source "$apppath/interfe/desktop"
source "$apppath/interfe/termine"



## USER PROJECTS
if [[ $debuger == true ]]; then
    mkdir -p "$tespath/systes/"{preset,builds}
    usrdirs="$tespath/systes/builds"
    usrconf="$tespath/systes/preset"
else
    
    mkdir -p "$HOME/.local/craine/"{preset,profile}
    usrmake="$HOME/.local/craine/builds"
    usrsets="$HOME/.local/craine/preset"
fi


## LOAD PROCCESS
source "$apppath/maker"

