#!/bin/bash

set -e


## LOAD PATHERS


applibs="$apppath/library"
appsrcs="$apppath/source"
appcore="$apppath/coresy"
apphelp="$apppath/helper"
inetcon=$( ping -q -c1 google.com &>/dev/null && echo true || echo false )



## HELPER LIBRARY
source "$apphelp/extralib"
source "$apphelp/desktop"


## USER PROJECTS
if [[ $debuger == true ]]; then
    mkdir -p "$tespath/systes"
    initdir="$tespath/systes"
else
    
    mkdir -p "$HOME/.local/craine/"
    initdir="$HOME/.local/craine"
fi



## LOAD PROCCESS
source "$apppath/maker"

